{
  "manifest_version": 2,
  "name": "WeFav",
  "description": "Manage my favorite WeWork spots",
  "version": "0.9",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.wework.com/*"],
      "css": ["css/button.css"],
      "js": [
        "javascripts/config/config.js", 
        "javascripts/config/models.js", 
        "javascripts/classes/helper.js",
        "javascripts/classes/db.js", 
        "javascripts/classes/city.js",
        "javascripts/button.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "storage",
    "*://www.wework.com/*"
  ]
}